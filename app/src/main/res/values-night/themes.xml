<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.MessageCat" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="android:forceDarkAllowed">false</item>

        <item name="android:colorPrimary">@color/design_default_color_primary</item>
        <item name="android:colorSecondary">@color/design_default_color_secondary</item>
    </style>

    <style name="Theme.MessageCat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MessageCat.AppBarOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.MessageCat.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>